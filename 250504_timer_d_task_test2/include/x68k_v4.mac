		bclr.b	#1,$e82600
		bclr.b	#2,$e82600
		bclr.b	#3,$e82600
		bclr.b	#4,$e82600
		bclr.b	#6,$e82600
		.endm

;ｸﾞﾗﾌｨｯｸﾍﾞｰｽ,ﾃｷｽﾄ間
GRTX_SEET_ON:	.macro
		move.b	#%00011001,$e82600
		.endm

GRTX_SEET_OFF:	.macro
		clr.b	$e82600
		.endm

;ｸﾞﾗﾌｨｯｸﾍﾞｰｽ,ｸﾞﾗﾌｨｯｸ間
GR_SEET_ON:	.macro
		move.b	#%00011110,$e82600
		.endm

GR_SEET_OFF:	.macro
		clr.b	$e82600
		.endm

CONTRA_SET:	.macro	data
		move.b	data,$e8e001
		.endm

CONTRA_INIT:	.macro
		move.b	#15,$e8e001
		.endm

CONTRA_DEC:	.macro
		move.b	$e8e001,d0
		subq.b	#1,d0
		move.b	d0,$e8e001
		.endm

CONTRA_INC:	.macro
		move.b	$e8e001,d0
		addq.b	#1,d0
		move.b	d0,$e8e001
		.endm
;------------------------------------------------------------------------------
BITSNS:		.macro	n		;IOCS	_BITSNSと同等に機能する
		move.b	$800+n.w,d0
		.endm

ABITSNS:	.macro	n,reg		;IOCS	_BITSNSと同等に機能する
		move.b	$800+n.w,reg
		.endm

AKEYSNS:	.macro	n,m
		btst.b	#m,$800+n.w
		.endm

KEY_MACHI:	.macro	n,m
		.local	lp
lp:		btst.b	#m,$800+n.w
		beq	lp
		.endm

;
;	reg.w	< 0なら		reg.w = $ffff
;		== 0なら	reg.w = 0
;		> 0なら		reg.w = 1
;	BASICのSGN()と同じ処理
;
sgn_w:		.macro	reg
		add.w	reg,reg
		shi	reg
		negx.b	reg
		ext.w	reg
		.endm

lsl_w8:		.macro	reg		;lsl.w	#8,regと同等に機能する
		move.b	reg,-(sp)
		move.w	(sp)+,reg
		.endm

sub2d_w:	.macro	r1,r2,r3
		.local	PL
		move.w	r1,r3
		sub.w	r2,r3
		bpl	PL
		neg.w	r3
PL:
		.endm

exg2_w:		.macro	r2,r1
		.local	PL
		cmp.w	r2,r1
		bpl	PL
		exg	r2,r1
PL:
		.endm
;------------------------------------------------------------------------------
push_l:		.macro	reg
		move.l	reg,-(sp)
		.endm

push_w:		.macro	reg
		move.w	reg,-(sp)
		.endm

pop_l:		.macro	reg
		move.l	(sp)+,reg
		.endm

pop_w:		.macro	reg
		move.w	(sp)+,reg
		.endm

push:		.macro	reg
		movem.l	reg,-(sp)
		.endm

pop:		.macro	reg
		movem.l	(sp)+,reg
		.endm
;------------------------------------------------------------------------------
		.list
