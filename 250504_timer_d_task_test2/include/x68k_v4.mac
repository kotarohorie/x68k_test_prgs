CRTCRAS_RMV:	.macro
		movem.l	d0/a1,-(sp)
		move.w	SR,-(sp)
		ori	#$2700,SR	; MPU割り込みﾏｽｸ
		suba.l	a1,a1
		IOCS	_CRTCRAS	; RAS割り込み停止
		move.w	(sp)+,SR
		movem.l	(sp)+,d0/a1
		.endm
