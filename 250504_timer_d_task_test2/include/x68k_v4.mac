kisen_machi:	.macro
		.local	VDISP,VDISP2,VDISP3,VDISP4,VDISP5
   if	dbg01<>0
		PUSH_L	d0
		IOCS	_B_SFTSNS
		btst	#$A,d0			;[ｺｰﾄﾞ]
		movem.l	(sp)+,d0
		beq	VDISP5
		move.w	#%00000_00000_00000_0,TX_PAL_ADDR
VDISP5:
		movem.l	d0-d1,-(sp)
		move.w	#2,d1
		IOCS	_BITSNS
		btst	#0,d0			;[TAB]
		movem.l	(sp)+,d0-d1
		bne	VDISP3
   endif
VDISP:		btst.b	#4,$e88001
		beq	VDISP
VDISP2:		btst.b	#4,$e88001
		bne	VDISP2
VDISP3:
   if	dbg01<>0
		PUSH_L	d0
		IOCS	_B_SFTSNS
		btst	#$A,d0			;[ｺｰﾄﾞ]
		movem.l	(sp)+,d0
		beq	VDISP4
		move.w	#%00000_00000_11111_0,TX_PAL_ADDR
VDISP4:
   endif
		.endm
